{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [{
        "source": "**/*.@(eot|otf|ttf|ttc|woff|woff2)",
        "headers": [{
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          },
          {
            "key": "Cache-Control",
            "value": "max-age=38880000"
          }
        ]
      }, {
        "source": "**/*.@(jpg|jpeg|gif|png|svg)",
        "headers": [{
          "key": "Cache-Control",
          "value": "max-age=38880000"
        }]
      },
      {
        "source": "**/*.@(js|css|json|webmanifest)",
        "headers": [{
          "key": "Cache-Control",
          "value": "max-age=604800"
        }]
      }, {
        "source": "404.html",
        "headers": [{
          "key": "Cache-Control",
          "value": "max-age=300"
        }]
      }
    ],
    "redirects": [
      {
        "source": "/index.html",
        "destination": "/",
        "type": 301
      },
      {
        "source": "blog.alokprateek.in/",
        "destination": "/",
        "type": 301
      },
      {
        "source": "/recent/",
        "destination": "/",
        "type": 301
      },
      {
        "source": "/recent/page2/",
        "destination": "/",
        "type": 301
      },
      {
      "source": "/privacy.html",
      "destination": "/terms/",
      "type": 301
      },
      {
        "source": "/terms.html",
